{
  "manifest_version": 3,
  "name": "Actuarial Present Value",
  "description": "Actuarial Present Value Calculator",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Actuarial Present Value",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["tabs", "bookmarks", "storage", "identity"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
